#!/usr/bin/env python

import timeit
print timeit.timeit('1+1', number=    10000) #      10,000
print timeit.timeit('1+1', number=  1000000) #   1,000,000
print timeit.timeit('1+1', number=100000000) # 100,000,000

# example result:

# 0.000196933746338 # 196 microseconds for     10,000 runs
# 0.0195851325989   # 19.6 milliseconds for 1,000,000 runs
# 1.96882104874     # 1.97 seconds for    100,000,000 runs
